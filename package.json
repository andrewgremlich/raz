{
  "name": "raz",
  "version": "0.4.0",
  "description": "A server based application with the intention of being served over wifi on a raspberry pi.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./app_scripts/",
    "dev": "nodemon .",
    "start": "node index.js",
    "roll": "rollup -i ./app_scripts/main.js -f iife -o ./app_scripts/main-concat.js --sourcemap",
    "pro-script": "babel ./app_scripts/main-concat.js --out-file ./app/js/main.js",
    "uglify": "uglifyjs --screw-ie8 ./app/js/main.js -c -m --output ./app/js/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewgremlich/raz.git"
  },
  "keywords": [
    "raspberry",
    "pi",
    "game",
    "web",
    "base",
    "server",
    "js"
  ],
  "author": "Andrew Gremlich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewgremlich/raz/issues"
  },
  "homepage": "https://github.com/andrewgremlich/raz#readme",
  "dependencies": {
    "express": "^4.14.1",
    "socket.io": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.22.0",
    "nodemon": "^1.11.0",
    "rollup": "^0.41.4",
    "uglify-js": "^2.7.5"
  }
}
